/* maps.wxss */

page {
  background: #f6f6f6;
}

.page-body {
  background: #f6f6f6;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  width: 100%;
  height: 100%;
}

text {
  align-items: left;
  justify-content: left;
  margin-left: 20rpx;
  font-size: 30rpx;
  background-color: #f6f6f6;
}

button {
  margin: 20rpx;
}

#map {
  width: 95%; 
  height: 310px;
  margin-top: 20rpx;
  margin-left: 20rpx;
  border: 1px #333 solid;
  border-radius: 5px;
}